{
  "name": "toc-lang",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://raymyers.github.io/toc-lang",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}' --ignore-pattern src/generated/",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'  --ignore-pattern src/generated/",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.json",
    "test": "NODE_ENV=test vitest run",
    "coverage": "NODE_ENV=test vitest run --coverage",
    "gen": "lezer-generator src/assets/grammars/toc-lang.lezer -o src/generated/tocLangLezerParser.js"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.4",
    "@column-resizer/react": "^1.3.1",
    "@lezer/lr": "^1.4.2",
    "@uiw/react-codemirror": "^4.23.7",
    "browser-fs-access": "^0.35.0",
    "canvg": "^4.0.1",
    "d3": "7.8.5",
    "dagre-d3-es": "7.0.10",
    "peggy": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@lezer/generator": "^1.7.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "approvals": "^7.0.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "3.2.5",
    "typescript": "^5.3.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.6.0"
  }
}
